{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "framework": "vite",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://127.0.0.1:5000/$1"
    }
  ],
  "regions": ["iad1"]
}

